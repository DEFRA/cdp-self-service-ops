{
  "name": "cdp-self-service-ops",
  "version": "0.33.0",
  "description": "Automates create and deployment of services",
  "main": ".server/index.js",
  "type": "module",
  "engines": {
    "node": ">=22.13.1"
  },
  "scripts": {
    "build": "npm run build:server",
    "build:server": "NODE_ENV=production babel --delete-dir-on-start --ignore \"**/*.test.js\" --ignore \"**/*.e2e.js\" --ignore \"**/__fixtures__\" --ignore \"**/test-helpers\" --copy-files --no-copy-ignored --out-dir ./.server ./src",
    "docker:dev": "NODE_ENV=development npm run server:watch",
    "dev": "NODE_ENV=development npm run server:watch",
    "dev:debug": "NODE_ENV=development npm run server:debug",
    "format": "prettier --write 'src/**/*.js' '**/*.{js,cjs,md,json,config.js,test.js}'",
    "format:check": "prettier --check 'src/**/*.js' '**/*.{js,cjs,md,json,config.js,test.js}'",
    "postinstall": "npm run setup:husky",
    "lint": "run-s lint:js lint:types",
    "lint:types": "tsc --build tsconfig.json",
    "lint:fix": "eslint --cache --fix .",
    "lint:js": "eslint --cache .",
    "postversion": "git add package.json package-lock.json && git commit -m $npm_package_version",
    "test": "TZ=UTC jest --coverage --verbose",
    "test:watch": "TZ=UTC jest --watch",
    "server:watch": "nodemon --exec tsx --enable-source-maps ./src",
    "server:debug": "nodemon --exec tsx --enable-source-maps --inspect=9239 ./src",
    "prestart": "npm run build",
    "start": "NODE_ENV=production node --use-strict .",
    "setup:husky": "node -e \"try { (await import('husky')).default() } catch (e) { if (e.code !== 'ERR_MODULE_NOT_FOUND') throw e }\" --input-type module"
  },
  "author": "Defra DDTS",
  "license": "OGL-UK-3.0",
  "dependencies": {
    "@aws-sdk/client-sns": "^3.723.0",
    "@aws-sdk/client-sqs": "^3.723.0",
    "@babel/runtime": "^7.26.0",
    "@defra/hapi-tracing": "^1.0.0",
    "@elastic/ecs-pino-format": "^1.5.0",
    "@hapi/boom": "^10.0.1",
    "@hapi/hapi": "^21.3.12",
    "@hapi/jwt": "^3.2.0",
    "@hapi/wreck": "^18.1.0",
    "@octokit/auth-app": "^7.1.4",
    "@octokit/core": "^6.1.3",
    "@octokit/graphql": "^8.1.2",
    "@octokit/plugin-rest-endpoint-methods": "^13.3.0",
    "aws4": "^1.13.2",
    "babel-plugin-module-resolver": "^5.0.2",
    "convict": "^6.2.4",
    "global-agent": "^3.0.0",
    "hapi-pino": "^12.1.0",
    "hapi-pulse": "^3.0.1",
    "https-proxy-agent": "^7.0.6",
    "joi": "^17.13.3",
    "lodash": "^4.17.21",
    "mongo-locks": "^3.0.2",
    "mongodb": "^6.12.0",
    "node-fetch": "^3.3.2",
    "npm-check-updates": "^17.1.13",
    "octokit-plugin-create-pull-request": "^6.0.0",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "qs": "^6.13.1",
    "sqs-consumer": "^11.4.0",
    "timeunit": "^1.1.1",
    "undici": "^7.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.0",
    "@babel/node": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@shelf/jest-mongodb": "^4.3.2",
    "@tsconfig/node22": "22.0.0",
    "@types/convict": "^6.1.6",
    "@types/eslint": "9.6.1",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.14",
    "@types/node": "^22.10.5",
    "@types/node-fetch": "^2.6.12",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.2.1",
    "babel-plugin-transform-import-meta": "^2.3.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.6.0",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-jsdoc": "^48.8.3",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^6.2.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.4.2",
    "resolve-url-loader": "^5.0.0",
    "timers": "^0.1.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}
